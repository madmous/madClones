{
  "name": "trello-clone-api",
  "version": "0.1.0",
  "author": "Moustapha Amadou Diouf",
  "license": "MIT",
  "main": "./src/index.js",
  "scripts": {
    "build": "babel ./src -d dist",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=dev nodemon ./src/index --exec babel-node",
    "start:debug": "NODE_ENV=dev babel-node ./src/index --debug-brk --inspect",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha ./src/api/**/*.spec.js --compilers js:babel-register",
    "docker-test": "NODE_ENV=docker-test ./node_modules/.bin/mocha ./src/api/**/*.spec.js --compilers js:babel-register",
    "test:debug": "NODE_ENV=test ./node_modules/.bin/mocha ./test --recursive --compilers js:babel-register --inspect --recursive --debug-brk",
    "test:cover": "NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly ./src/api/**/*.spec.js -- --require babel-register",
    "docker-test:cover": "NODE_ENV=docker-test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha ./src/api/**/*.spec.js -- --require babel-register"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "sinon": "^2.2.0",
    "sinon-mongoose": "^1.3.0"
  },
  "dependencies": {
    "async": "^2.1.2",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "boom": "^4.3.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "csurf": "^1.9.0",
    "express": "^4.14.0",
    "express-validation": "^1.0.2",
    "global": "^4.3.1",
    "helmet": "^3.1.0",
    "joi": "^10.3.3",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.11",
    "mongoose": "^4.7.0",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "request": "^2.81.0",
    "winston": "^2.3.0"
  },
  "engines": {
    "node": "7.8.0"
  },
  "proxy": "http:://localhost"
}
